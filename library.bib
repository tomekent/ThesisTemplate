journal = {The Journal of chemical physics},
month = mar,
number = {12},
pages = {124706 1--9},
pmid = {21456694},
title = {{Triplet states of zigzag edged hexagonal graphene molecules C(6m**2)H(6m) (m = 1, 2, 3, ..., 10) and carbon based magnetism.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21456694},
